<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/houseowner_info_add_title"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:focusable="true"
        android:focusableInTouchMode="true" >

        <Button
            android:id="@+id/houseowner_info_add_back"
         style="@style/Back_Button" />

        <TextView
            android:id="@+id/houseowner_info_add_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:singleLine="true"
            android:text="完善成员资料"
            android:textColor="@color/text_title"
            android:textSize="20sp" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dip"
        android:background="@color/line_div" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dip"
            android:layout_weight="2"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="1"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="2.5"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="right|center_vertical"
                            android:lines="1"
                            android:paddingRight="15dip"
                            android:text="姓名"
                            android:textColor="@color/text_title"
                            android:textSize="@dimen/textsize_16" />
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="7"
                        android:orientation="horizontal" >

                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="30dip"
                            android:layout_centerVertical="true"
                            android:background="@color/line_div" />

                        <EditText
                            android:id="@+id/houseowner_info_add_name_text"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:gravity="center|left"
                            android:hint="请填写姓名"
                            android:inputType="phone"
                            android:lines="1"
                            android:maxLength="11"
                            android:paddingLeft="15dip"
                            android:textColor="@color/text_title"
                            android:textColorHint="@color/text_hint_normal"
                            android:textSize="@dimen/textsize_16" />

                        <Button
                            android:id="@+id/select_user"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dp"
                            android:background="@drawable/m_register"
                            android:textColor="@color/white" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_div" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="1"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="right|center_vertical"
                            android:paddingRight="15dip"
                            android:text="手机"
                            android:textColor="@color/text_title"
                            android:textSize="@dimen/textsize_16" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_gravity="left"
                        android:layout_weight="7"
                        android:orientation="horizontal" >

                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="30dip"
                            android:layout_gravity="center"
                            android:background="@color/line_div" />

                        <EditText
                            android:id="@+id/houseowner_info_add_phone_text"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:gravity="center|left"
                            android:hint="请输入手机号码"
                            android:paddingLeft="15dip"
                            android:textColor="@color/text_title"
                            android:textColorHint="@color/text_hint_normal"
                            android:textSize="@dimen/textsize_16" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_div" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="1"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/houseowner_info_add_sex_title"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="right|center_vertical"
                            android:paddingRight="15dip"
                            android:text="性别"
                            android:textColor="@color/text_title"
                            android:textSize="@dimen/textsize_16" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="7"
                        android:orientation="horizontal" >

                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="30dip"
                            android:layout_gravity="center"
                            android:background="@color/line_div" />

                        <LinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="7"
                            android:gravity="center|left"
                            android:orientation="horizontal"
                            android:paddingLeft="10dip" >

                            <RadioGroup
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/houseowner_info_add_sex_line"
                                android:gravity="center"
                                android:orientation="horizontal" >

                                <RadioButton
                                    android:id="@+id/houseowner_info_add_sex_man"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_toRightOf="@+id/houseowner_info_add_sex_line"
                                    android:button="@null"
                                    android:checked="true"
                                    android:drawableLeft="@drawable/style_select_sex"
                                    android:gravity="center" />

                                <TextView
                                    android:id="@+id/tt_nan"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_toRightOf="@+id/houseowner_info_add_sex_line"
                                    android:paddingLeft="5.5dip"
                                    android:text="男"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/textsize_16" />

                                <RadioButton
                                    android:id="@+id/houseowner_info_add_sex_woman"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="25dp"
                                    android:layout_toRightOf="@+id/houseowner_info_add_sex_man"
                                    android:button="@null"
                                    android:checked="false"
                                    android:drawableLeft="@drawable/style_select_sex" />

                                <TextView
                                    android:id="@+id/tt_nv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_toRightOf="@+id/houseowner_info_add_sex_line"
                                    android:paddingLeft="5.5dip"
                                    android:text="女"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/textsize_16" />
                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_div" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="1"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="2.5"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="right|center_vertical"
                            android:lines="1"
                            android:paddingRight="15dip"
                            android:text="住户类型"
                            android:textColor="@color/text_title"
                            android:textSize="@dimen/textsize_16" />
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="7" >

                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="30dip"
                            android:layout_centerVertical="true"
                            android:background="@color/line_div" />

                        <TextView
                            android:id="@+id/houseowner_info_add_status_text"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:gravity="center|left"
                            android:hint="请选择住户类型"
                            android:paddingLeft="15dip"
                            android:textColor="@color/text_title"
                            android:textColorHint="@color/text_hint_normal"
                            android:textSize="@dimen/textsize_16" />

                        <ImageView
                            android:id="@+id/repairs_private_apply_item_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="20dp"
                            android:src="@drawable/home_icon_cbb" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="@color/line_div" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dip"
            android:layout_weight="3"
            android:background="@color/colorBg"
            android:orientation="vertical" >

            <Button
                android:id="@+id/houseowner_info_add_commit"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="40dp"
                android:background="@drawable/button_common"
                android:gravity="center"
                android:text="保存"
                android:textColor="@color/white"
                android:textSize="18sp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>